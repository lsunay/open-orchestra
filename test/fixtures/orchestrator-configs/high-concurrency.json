{
  "$schema": "../../../schema/orchestrator.schema.json",
  "basePort": 19000,
  "autoSpawn": true,
  "startupTimeout": 60000,
  "healthCheckInterval": 5000,
  "ui": {
    "toasts": false,
    "injectSystemContext": true,
    "systemContextMaxWorkers": 20,
    "defaultListFormat": "json",
    "firstRunDemo": false
  },
  "notifications": {
    "idle": {
      "enabled": false
    }
  },
  "agent": {
    "enabled": true,
    "name": "stress-orchestrator",
    "mode": "primary"
  },
  "commands": {
    "enabled": true,
    "prefix": "stress."
  },
  "pruning": {
    "enabled": true,
    "maxToolOutputChars": 10000,
    "maxToolInputChars": 5000,
    "protectedTools": []
  },
  "profiles": [
    {
      "id": "stress-worker-1",
      "name": "Stress Worker 1",
      "model": "test-model",
      "purpose": "High concurrency stress testing",
      "whenToUse": "For stress tests with concurrent spawns",
      "temperature": 0.1
    },
    {
      "id": "stress-worker-2",
      "name": "Stress Worker 2",
      "model": "test-model",
      "purpose": "High concurrency stress testing",
      "whenToUse": "For stress tests with concurrent spawns",
      "temperature": 0.1
    },
    {
      "id": "stress-worker-3",
      "name": "Stress Worker 3",
      "model": "test-model",
      "purpose": "High concurrency stress testing",
      "whenToUse": "For stress tests with concurrent spawns",
      "temperature": 0.1
    },
    {
      "id": "stress-worker-4",
      "name": "Stress Worker 4",
      "model": "test-model",
      "purpose": "High concurrency stress testing",
      "whenToUse": "For stress tests with concurrent spawns",
      "temperature": 0.1
    },
    {
      "id": "stress-worker-5",
      "name": "Stress Worker 5",
      "model": "test-model",
      "purpose": "High concurrency stress testing",
      "whenToUse": "For stress tests with concurrent spawns",
      "temperature": 0.1
    }
  ],
  "workers": [
    "stress-worker-1",
    "stress-worker-2",
    "stress-worker-3"
  ]
}
